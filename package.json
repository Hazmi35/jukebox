    "opusscript": "^0.0.8",
    "plur": "^4.0.0",
    "pretty-ms": "^7.0.1",
    "prism-media": "^1.3.2",
    "strip-indent": "^3.0.0",
    "tslib": "^2.3.1",
    "winston": "^3.3.3",
