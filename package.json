{
  "name": "rendang",
  "version": "2.1.0",
  "description": " A (maybe) useful bot for your Discord server!",
  "main": "src/main.ts",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js)\"",
    "lint:fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js)\" --fix",
    "test": "pnpm run lint",
    "build": "pnpm run test && rimraf dist && tsc",
    "start": "node dist/main",
    "start:dev": "ts-node src/main"
  },
  "engines": {
    "node": ">=12.x"
  },
  "author": "Hazmi35 (contact@hzmi.xyz) (https://github.com/Hazmi35)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Hazmi35/rendang/issues"
  },
  "homepage": "https://github.com/Hazmi35/rendang#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "winston": "^3.2.1"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.1.0",
    "bufferutil": "^4.0.1",
    "erlpack": "github:discordapp/erlpack",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.6"
  },
  "devDependencies": {
    "@types/node": "^12.12.32",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
